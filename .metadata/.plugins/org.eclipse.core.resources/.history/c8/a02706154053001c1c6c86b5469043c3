<script type="text/javascript">
	function fun1() {
		alert("Hello");
	}

	function fun2(a, b) {
		alert(2 * a + 3 * b);
	}

	function fun3(a, b, c) {
		alert("세 수의 합")
		return a + b + c;

	}
	fun1(); //메시지출력
	fun2(10, 20); //두수의 곱출력
	alert(fun3(80, 95, 90)); //세 수를 더해서 리턴
	
	function name() {
		console.log("L")
	}
	name();
</script>
