<script type="text/javascript">
	// 	내장함수 자바스크립트에 있는 함수

	// 메세지 출력하는 내장함수 호출
	// alert(10);

	// 내장함수 호출시 값을 입력받아서 
	var a = prompt(); //입력값은 문자열로
	alert(a + 22); //문자열로 
	alert(parseInt(a) + 22); //정수로 계산가능
	alert(parseFloat(a) + 22); //실수형\
</script>
